# Server Configuration
PORT=3000
NODE_ENV=development

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Mock Mode (for development without API keys/browsers)
MOCK_CRAWLER=true
MOCK_AI=true

# Performance Tuning
CACHE_TTL=3600 # Reference message cache TTL in seconds (1 hour)
CRAWLER_PARALLEL_JOBS=3 # Number of concurrent crawler jobs
REQUEST_TIMEOUT=30000 # Request timeout in milliseconds (30 seconds)

# Logging
LOG_LEVEL=info